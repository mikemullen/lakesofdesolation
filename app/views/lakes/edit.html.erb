<h1>Edit <%= @lake.name %></h1>
<%= form_for(@lake) do |f| %>

  <p>
    <%= f.label :filename %><br>
    <%= f.text_field :filename %>
  </p>

  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :alphaname %><br>
    <%= f.text_field :alphaname %>
  </p>

  <p>
    <%= f.label :visited %>
    <%= f.check_box :visited %><br>
  </p>

  <p>
  	<%= f.submit "Update" %>
  </p>
<% end %>