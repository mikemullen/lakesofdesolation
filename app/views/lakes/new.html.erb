<h1>Add a new Lake</h1>
<%= form_for :lake, url: lakes_path do |f| %>
  <p>
  	<%= f.label :name %><br>
  	<%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :alphaname %><br>
    <%= f.text_field :alphaname %>
  </p>

  <p>
    <%= f.label :filename %><br>
    <%= f.text_field :filename %>
  </p>

  <p>
    <%= f.label :visited %>
    <%= f.check_box :visited %><br>
  </p>

  <p>
  	<%= f.submit "Post" %>
  </p>
<% end %>