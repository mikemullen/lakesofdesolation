<% provide(:title, @lake.name) %>
<% provide(:file_name, @lake.filename) %>
<h1><%= @lake.name %></h1>
<%= link_to(@prevlake) do %>
   <div id="prevbutton"></div>
<% end %>

<%= link_to(@nextlake) do %>
   <div id="nextbutton"></div>
<% end %>

<% if signed_in? %>
<%= link_to "delete", @lake, method: :delete, data: { confirm: "You sure?" } %> | 
<%= link_to "edit", edit_lake_path(@lake) %>
<% end %>
